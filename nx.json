{
  "npmScope": "client-platform",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e"
        ]
      }
    }
  },
  "projects": {
    "core": {
      "tags": []
    },
    "core-ui": {
      "tags": []
    },
    "core-translate": {
      "tags": []
    },
    "example-app": {
      "tags": []
    },
    "example-app-e2e": {
      "tags": [],
      "implicitDependencies": [
        "example-app"
      ]
    }
  }
}
